{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#10141c90",
                "editor.background": "#10141c90",
                "editor.foreground": "#c3c4c6",
                "text": "#c3c4c6",
                "text.muted": "#c3c4c670",
                "text.ignored": "#c3c4c640",
                "text.placeholder": "#c3c4c650",
                "ignored": "#c3c4c630",
                "element.hover": "#c3c4c630",
                "ghost_element.hover": "#49485730",
                "ghost_element.selected": "#49485730",
                "ghost_element.active": "#49485760",
                "border": "#10141c",
                "editor.highlighted_line.background": "#49485710",
                "editor.active_line.background": "#49485710",
                "panel.background": "#10141c90",
                "title_bar.background": "#10141c90",
                "title_bar.inactive_background": "#10141c90",
                "status_bar.background": "#10141c90",
                "drop_target.background": "#10141c90",
                "elevated_surface.background": "#10141c",
                "toolbar.background": "#10141c90",
                "tab_bar.background": "#10141c90",
                "tab.inactive_background": "#10141c90",
                "tab.active_background": "#49485730",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#10141c",
                "scrollbar.thumb.background": "#494857",
                "editor.gutter.background": "#10141c90",
                "terminal.background": "#10141c10",
                "terminal.foreground": "#c3c4c6",
                "terminal.dim_foreground": "#c3c4c6",
                "terminal.bright_foreground": "#c3c4c6",
                "terminal.ansi.black": "#10141c",
                "terminal.ansi.red": "#494857",
                "terminal.ansi.green": "#4A576C",
                "terminal.ansi.yellow": "#5C5A6C",
                "terminal.ansi.blue": "#A5706A",
                "terminal.ansi.magenta": "#566D8D",
                "terminal.ansi.cyan": "#637A9D",
                "terminal.ansi.white": "#c3c4c6",
                "terminal.ansi.bright_black": "#5c6471",
                "terminal.ansi.bright_red": "#494857",
                "terminal.ansi.bright_green": "#4A576C",
                "terminal.ansi.bright_yellow": "#5C5A6C",
                "terminal.ansi.bright_blue": "#A5706A",
                "terminal.ansi.bright_magenta": "#566D8D",
                "terminal.ansi.bright_cyan": "#637A9D",
                "terminal.ansi.bright_white": "#c3c4c6",
                "modified": "#494857",
                "syntax": {
                    "attribute": {
                        "color": "#494857"
                    },
                    "boolean": {
                      "color": "#4A576C"
                    },
                    "comment": {
                      "color": "#5c6471"
                    },
                    "comment.doc": {
                      "color": "#5c6471"
                    },
                    "constant": {
                      "color": "#4A576C"
                    },
                    "function": {
                      "color": "#637A9D"
                    },
                    "keyword": {
                      "color": "#A5706A"
                    },
                    "number": {
                      "color": "#566D8D"
                    },
                    "operator": {
                      "color": "#A5706A"
                    },
                    "string": {
                      "color": "#494857"
                    },
                    "variable": {
                      "color": "#4A576C"
                    }
                },
                "players": [
                  {
                    "cursor": "#494857",
                    "background": "#10141c",
                    "selection": "#49485730"
                  }
                ]
            }
        }
    ]
}
