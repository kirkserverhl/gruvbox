{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#11131390",
                "editor.background": "#11131390",
                "editor.foreground": "#c3c4c4",
                "text": "#c3c4c4",
                "text.muted": "#c3c4c470",
                "text.ignored": "#c3c4c440",
                "text.placeholder": "#c3c4c450",
                "ignored": "#c3c4c430",
                "element.hover": "#c3c4c430",
                "ghost_element.hover": "#65615830",
                "ghost_element.selected": "#65615830",
                "ghost_element.active": "#65615860",
                "border": "#111313",
                "editor.highlighted_line.background": "#65615810",
                "editor.active_line.background": "#65615810",
                "panel.background": "#11131390",
                "title_bar.background": "#11131390",
                "title_bar.inactive_background": "#11131390",
                "status_bar.background": "#11131390",
                "drop_target.background": "#11131390",
                "elevated_surface.background": "#111313",
                "toolbar.background": "#11131390",
                "tab_bar.background": "#11131390",
                "tab.inactive_background": "#11131390",
                "tab.active_background": "#65615830",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#111313",
                "scrollbar.thumb.background": "#656158",
                "editor.gutter.background": "#11131390",
                "terminal.background": "#11131310",
                "terminal.foreground": "#c3c4c4",
                "terminal.dim_foreground": "#c3c4c4",
                "terminal.bright_foreground": "#c3c4c4",
                "terminal.ansi.black": "#111313",
                "terminal.ansi.red": "#656158",
                "terminal.ansi.green": "#6C7268",
                "terminal.ansi.yellow": "#B15A44",
                "terminal.ansi.blue": "#788873",
                "terminal.ansi.magenta": "#ABA06F",
                "terminal.ansi.cyan": "#848881",
                "terminal.ansi.white": "#c3c4c4",
                "terminal.ansi.bright_black": "#5a6e6e",
                "terminal.ansi.bright_red": "#656158",
                "terminal.ansi.bright_green": "#6C7268",
                "terminal.ansi.bright_yellow": "#B15A44",
                "terminal.ansi.bright_blue": "#788873",
                "terminal.ansi.bright_magenta": "#ABA06F",
                "terminal.ansi.bright_cyan": "#848881",
                "terminal.ansi.bright_white": "#c3c4c4",
                "modified": "#656158",
                "syntax": {
                    "attribute": {
                        "color": "#656158"
                    },
                    "boolean": {
                      "color": "#6C7268"
                    },
                    "comment": {
                      "color": "#5a6e6e"
                    },
                    "comment.doc": {
                      "color": "#5a6e6e"
                    },
                    "constant": {
                      "color": "#6C7268"
                    },
                    "function": {
                      "color": "#848881"
                    },
                    "keyword": {
                      "color": "#788873"
                    },
                    "number": {
                      "color": "#ABA06F"
                    },
                    "operator": {
                      "color": "#788873"
                    },
                    "string": {
                      "color": "#656158"
                    },
                    "variable": {
                      "color": "#6C7268"
                    }
                },
                "players": [
                  {
                    "cursor": "#656158",
                    "background": "#111313",
                    "selection": "#65615830"
                  }
                ]
            }
        }
    ]
}
