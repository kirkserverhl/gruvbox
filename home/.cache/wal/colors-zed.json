{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0E131890",
                "editor.background": "#0E131890",
                "editor.foreground": "#c2c4c5",
                "text": "#c2c4c5",
                "text.muted": "#c2c4c570",
                "text.ignored": "#c2c4c540",
                "text.placeholder": "#c2c4c550",
                "ignored": "#c2c4c530",
                "element.hover": "#c2c4c530",
                "ghost_element.hover": "#6C726930",
                "ghost_element.selected": "#6C726930",
                "ghost_element.active": "#6C726960",
                "border": "#0E1318",
                "editor.highlighted_line.background": "#6C726910",
                "editor.active_line.background": "#6C726910",
                "panel.background": "#0E131890",
                "title_bar.background": "#0E131890",
                "title_bar.inactive_background": "#0E131890",
                "status_bar.background": "#0E131890",
                "drop_target.background": "#0E131890",
                "elevated_surface.background": "#0E1318",
                "toolbar.background": "#0E131890",
                "tab_bar.background": "#0E131890",
                "tab.inactive_background": "#0E131890",
                "tab.active_background": "#6C726930",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0E1318",
                "scrollbar.thumb.background": "#6C7269",
                "editor.gutter.background": "#0E131890",
                "terminal.background": "#0E131810",
                "terminal.foreground": "#c2c4c5",
                "terminal.dim_foreground": "#c2c4c5",
                "terminal.bright_foreground": "#c2c4c5",
                "terminal.ansi.black": "#0E1318",
                "terminal.ansi.red": "#6C7269",
                "terminal.ansi.green": "#7C8375",
                "terminal.ansi.yellow": "#888A7A",
                "terminal.ansi.blue": "#A7A992",
                "terminal.ansi.magenta": "#C0BFA3",
                "terminal.ansi.cyan": "#D6D5B4",
                "terminal.ansi.white": "#c2c4c5",
                "terminal.ansi.bright_black": "#5a646f",
                "terminal.ansi.bright_red": "#6C7269",
                "terminal.ansi.bright_green": "#7C8375",
                "terminal.ansi.bright_yellow": "#888A7A",
                "terminal.ansi.bright_blue": "#A7A992",
                "terminal.ansi.bright_magenta": "#C0BFA3",
                "terminal.ansi.bright_cyan": "#D6D5B4",
                "terminal.ansi.bright_white": "#c2c4c5",
                "modified": "#6C7269",
                "syntax": {
                    "attribute": {
                        "color": "#6C7269"
                    },
                    "boolean": {
                      "color": "#7C8375"
                    },
                    "comment": {
                      "color": "#5a646f"
                    },
                    "comment.doc": {
                      "color": "#5a646f"
                    },
                    "constant": {
                      "color": "#7C8375"
                    },
                    "function": {
                      "color": "#D6D5B4"
                    },
                    "keyword": {
                      "color": "#A7A992"
                    },
                    "number": {
                      "color": "#C0BFA3"
                    },
                    "operator": {
                      "color": "#A7A992"
                    },
                    "string": {
                      "color": "#6C7269"
                    },
                    "variable": {
                      "color": "#7C8375"
                    }
                },
                "players": [
                  {
                    "cursor": "#6C7269",
                    "background": "#0E1318",
                    "selection": "#6C726930"
                  }
                ]
            }
        }
    ]
}
