{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#10141b90",
                "editor.background": "#10141b90",
                "editor.foreground": "#c3c4c6",
                "text": "#c3c4c6",
                "text.muted": "#c3c4c670",
                "text.ignored": "#c3c4c640",
                "text.placeholder": "#c3c4c650",
                "ignored": "#c3c4c630",
                "element.hover": "#c3c4c630",
                "ghost_element.hover": "#80807F30",
                "ghost_element.selected": "#80807F30",
                "ghost_element.active": "#80807F60",
                "border": "#10141b",
                "editor.highlighted_line.background": "#80807F10",
                "editor.active_line.background": "#80807F10",
                "panel.background": "#10141b90",
                "title_bar.background": "#10141b90",
                "title_bar.inactive_background": "#10141b90",
                "status_bar.background": "#10141b90",
                "drop_target.background": "#10141b90",
                "elevated_surface.background": "#10141b",
                "toolbar.background": "#10141b90",
                "tab_bar.background": "#10141b90",
                "tab.inactive_background": "#10141b90",
                "tab.active_background": "#80807F30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#10141b",
                "scrollbar.thumb.background": "#80807F",
                "editor.gutter.background": "#10141b90",
                "terminal.background": "#10141b10",
                "terminal.foreground": "#c3c4c6",
                "terminal.dim_foreground": "#c3c4c6",
                "terminal.bright_foreground": "#c3c4c6",
                "terminal.ansi.black": "#10141b",
                "terminal.ansi.red": "#80807F",
                "terminal.ansi.green": "#797C83",
                "terminal.ansi.yellow": "#7E8187",
                "terminal.ansi.blue": "#8B8E92",
                "terminal.ansi.magenta": "#A5A8AE",
                "terminal.ansi.cyan": "#BABEC5",
                "terminal.ansi.white": "#c3c4c6",
                "terminal.ansi.bright_black": "#5c6570",
                "terminal.ansi.bright_red": "#80807F",
                "terminal.ansi.bright_green": "#797C83",
                "terminal.ansi.bright_yellow": "#7E8187",
                "terminal.ansi.bright_blue": "#8B8E92",
                "terminal.ansi.bright_magenta": "#A5A8AE",
                "terminal.ansi.bright_cyan": "#BABEC5",
                "terminal.ansi.bright_white": "#c3c4c6",
                "modified": "#80807F",
                "syntax": {
                    "attribute": {
                        "color": "#80807F"
                    },
                    "boolean": {
                      "color": "#797C83"
                    },
                    "comment": {
                      "color": "#5c6570"
                    },
                    "comment.doc": {
                      "color": "#5c6570"
                    },
                    "constant": {
                      "color": "#797C83"
                    },
                    "function": {
                      "color": "#BABEC5"
                    },
                    "keyword": {
                      "color": "#8B8E92"
                    },
                    "number": {
                      "color": "#A5A8AE"
                    },
                    "operator": {
                      "color": "#8B8E92"
                    },
                    "string": {
                      "color": "#80807F"
                    },
                    "variable": {
                      "color": "#797C83"
                    }
                },
                "players": [
                  {
                    "cursor": "#80807F",
                    "background": "#10141b",
                    "selection": "#80807F30"
                  }
                ]
            }
        }
    ]
}
