{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#18181890",
                "editor.background": "#18181890",
                "editor.foreground": "#c5c5c5",
                "text": "#c5c5c5",
                "text.muted": "#c5c5c570",
                "text.ignored": "#c5c5c540",
                "text.placeholder": "#c5c5c550",
                "ignored": "#c5c5c530",
                "element.hover": "#c5c5c530",
                "ghost_element.hover": "#689D6A30",
                "ghost_element.selected": "#689D6A30",
                "ghost_element.active": "#689D6A60",
                "border": "#181818",
                "editor.highlighted_line.background": "#689D6A10",
                "editor.active_line.background": "#689D6A10",
                "panel.background": "#18181890",
                "title_bar.background": "#18181890",
                "title_bar.inactive_background": "#18181890",
                "status_bar.background": "#18181890",
                "drop_target.background": "#18181890",
                "elevated_surface.background": "#181818",
                "toolbar.background": "#18181890",
                "tab_bar.background": "#18181890",
                "tab.inactive_background": "#18181890",
                "tab.active_background": "#689D6A30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#181818",
                "scrollbar.thumb.background": "#689D6A",
                "editor.gutter.background": "#18181890",
                "terminal.background": "#18181810",
                "terminal.foreground": "#c5c5c5",
                "terminal.dim_foreground": "#c5c5c5",
                "terminal.bright_foreground": "#c5c5c5",
                "terminal.ansi.black": "#181818",
                "terminal.ansi.red": "#689D6A",
                "terminal.ansi.green": "#D69821",
                "terminal.ansi.yellow": "#988D73",
                "terminal.ansi.blue": "#437E80",
                "terminal.ansi.magenta": "#458588",
                "terminal.ansi.cyan": "#B9AD8F",
                "terminal.ansi.white": "#c5c5c5",
                "terminal.ansi.bright_black": "#725d5d",
                "terminal.ansi.bright_red": "#689D6A",
                "terminal.ansi.bright_green": "#D69821",
                "terminal.ansi.bright_yellow": "#988D73",
                "terminal.ansi.bright_blue": "#437E80",
                "terminal.ansi.bright_magenta": "#458588",
                "terminal.ansi.bright_cyan": "#B9AD8F",
                "terminal.ansi.bright_white": "#c5c5c5",
                "modified": "#689D6A",
                "syntax": {
                    "attribute": {
                        "color": "#689D6A"
                    },
                    "boolean": {
                      "color": "#D69821"
                    },
                    "comment": {
                      "color": "#725d5d"
                    },
                    "comment.doc": {
                      "color": "#725d5d"
                    },
                    "constant": {
                      "color": "#D69821"
                    },
                    "function": {
                      "color": "#B9AD8F"
                    },
                    "keyword": {
                      "color": "#437E80"
                    },
                    "number": {
                      "color": "#458588"
                    },
                    "operator": {
                      "color": "#437E80"
                    },
                    "string": {
                      "color": "#689D6A"
                    },
                    "variable": {
                      "color": "#D69821"
                    }
                },
                "players": [
                  {
                    "cursor": "#689D6A",
                    "background": "#181818",
                    "selection": "#689D6A30"
                  }
                ]
            }
        }
    ]
}
