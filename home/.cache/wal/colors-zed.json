{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#191d2490",
                "editor.background": "#191d2490",
                "editor.foreground": "#c5c6c8",
                "text": "#c5c6c8",
                "text.muted": "#c5c6c870",
                "text.ignored": "#c5c6c840",
                "text.placeholder": "#c5c6c850",
                "ignored": "#c5c6c830",
                "element.hover": "#c5c6c830",
                "ghost_element.hover": "#5E7CA130",
                "ghost_element.selected": "#5E7CA130",
                "ghost_element.active": "#5E7CA160",
                "border": "#191d24",
                "editor.highlighted_line.background": "#5E7CA110",
                "editor.active_line.background": "#5E7CA110",
                "panel.background": "#191d2490",
                "title_bar.background": "#191d2490",
                "title_bar.inactive_background": "#191d2490",
                "status_bar.background": "#191d2490",
                "drop_target.background": "#191d2490",
                "elevated_surface.background": "#191d24",
                "toolbar.background": "#191d2490",
                "tab_bar.background": "#191d2490",
                "tab.inactive_background": "#191d2490",
                "tab.active_background": "#5E7CA130",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#191d24",
                "scrollbar.thumb.background": "#5E7CA1",
                "editor.gutter.background": "#191d2490",
                "terminal.background": "#191d2410",
                "terminal.foreground": "#c5c6c8",
                "terminal.dim_foreground": "#c5c6c8",
                "terminal.bright_foreground": "#c5c6c8",
                "terminal.ansi.black": "#191d24",
                "terminal.ansi.red": "#5E7CA1",
                "terminal.ansi.green": "#708797",
                "terminal.ansi.yellow": "#7592A5",
                "terminal.ansi.blue": "#91999E",
                "terminal.ansi.magenta": "#AFB0AE",
                "terminal.ansi.cyan": "#C3BAB1",
                "terminal.ansi.white": "#c5c6c8",
                "terminal.ansi.bright_black": "#616a77",
                "terminal.ansi.bright_red": "#5E7CA1",
                "terminal.ansi.bright_green": "#708797",
                "terminal.ansi.bright_yellow": "#7592A5",
                "terminal.ansi.bright_blue": "#91999E",
                "terminal.ansi.bright_magenta": "#AFB0AE",
                "terminal.ansi.bright_cyan": "#C3BAB1",
                "terminal.ansi.bright_white": "#c5c6c8",
                "modified": "#5E7CA1",
                "syntax": {
                    "attribute": {
                        "color": "#5E7CA1"
                    },
                    "boolean": {
                      "color": "#708797"
                    },
                    "comment": {
                      "color": "#616a77"
                    },
                    "comment.doc": {
                      "color": "#616a77"
                    },
                    "constant": {
                      "color": "#708797"
                    },
                    "function": {
                      "color": "#C3BAB1"
                    },
                    "keyword": {
                      "color": "#91999E"
                    },
                    "number": {
                      "color": "#AFB0AE"
                    },
                    "operator": {
                      "color": "#91999E"
                    },
                    "string": {
                      "color": "#5E7CA1"
                    },
                    "variable": {
                      "color": "#708797"
                    }
                },
                "players": [
                  {
                    "cursor": "#5E7CA1",
                    "background": "#191d24",
                    "selection": "#5E7CA130"
                  }
                ]
            }
        }
    ]
}
