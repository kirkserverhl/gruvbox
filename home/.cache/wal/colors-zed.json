{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0f0f0f90",
                "editor.background": "#0f0f0f90",
                "editor.foreground": "#c3c3c3",
                "text": "#c3c3c3",
                "text.muted": "#c3c3c370",
                "text.ignored": "#c3c3c340",
                "text.placeholder": "#c3c3c350",
                "ignored": "#c3c3c330",
                "element.hover": "#c3c3c330",
                "ghost_element.hover": "#93373730",
                "ghost_element.selected": "#93373730",
                "ghost_element.active": "#93373760",
                "border": "#0f0f0f",
                "editor.highlighted_line.background": "#93373710",
                "editor.active_line.background": "#93373710",
                "panel.background": "#0f0f0f90",
                "title_bar.background": "#0f0f0f90",
                "title_bar.inactive_background": "#0f0f0f90",
                "status_bar.background": "#0f0f0f90",
                "drop_target.background": "#0f0f0f90",
                "elevated_surface.background": "#0f0f0f",
                "toolbar.background": "#0f0f0f90",
                "tab_bar.background": "#0f0f0f90",
                "tab.inactive_background": "#0f0f0f90",
                "tab.active_background": "#93373730",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0f0f0f",
                "scrollbar.thumb.background": "#933737",
                "editor.gutter.background": "#0f0f0f90",
                "terminal.background": "#0f0f0f10",
                "terminal.foreground": "#c3c3c3",
                "terminal.dim_foreground": "#c3c3c3",
                "terminal.bright_foreground": "#c3c3c3",
                "terminal.ansi.black": "#0f0f0f",
                "terminal.ansi.red": "#933737",
                "terminal.ansi.green": "#A23D3D",
                "terminal.ansi.yellow": "#994848",
                "terminal.ansi.blue": "#7B7F80",
                "terminal.ansi.magenta": "#7C8080",
                "terminal.ansi.cyan": "#8D8D8D",
                "terminal.ansi.white": "#c3c3c3",
                "terminal.ansi.bright_black": "#6c5959",
                "terminal.ansi.bright_red": "#933737",
                "terminal.ansi.bright_green": "#A23D3D",
                "terminal.ansi.bright_yellow": "#994848",
                "terminal.ansi.bright_blue": "#7B7F80",
                "terminal.ansi.bright_magenta": "#7C8080",
                "terminal.ansi.bright_cyan": "#8D8D8D",
                "terminal.ansi.bright_white": "#c3c3c3",
                "modified": "#933737",
                "syntax": {
                    "attribute": {
                        "color": "#933737"
                    },
                    "boolean": {
                      "color": "#A23D3D"
                    },
                    "comment": {
                      "color": "#6c5959"
                    },
                    "comment.doc": {
                      "color": "#6c5959"
                    },
                    "constant": {
                      "color": "#A23D3D"
                    },
                    "function": {
                      "color": "#8D8D8D"
                    },
                    "keyword": {
                      "color": "#7B7F80"
                    },
                    "number": {
                      "color": "#7C8080"
                    },
                    "operator": {
                      "color": "#7B7F80"
                    },
                    "string": {
                      "color": "#933737"
                    },
                    "variable": {
                      "color": "#A23D3D"
                    }
                },
                "players": [
                  {
                    "cursor": "#933737",
                    "background": "#0f0f0f",
                    "selection": "#93373730"
                  }
                ]
            }
        }
    ]
}
