{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0f0f0e90",
                "editor.background": "#0f0f0e90",
                "editor.foreground": "#c3c3c2",
                "text": "#c3c3c2",
                "text.muted": "#c3c3c270",
                "text.ignored": "#c3c3c240",
                "text.placeholder": "#c3c3c250",
                "ignored": "#c3c3c230",
                "element.hover": "#c3c3c230",
                "ghost_element.hover": "#C8AE3630",
                "ghost_element.selected": "#C8AE3630",
                "ghost_element.active": "#C8AE3660",
                "border": "#0f0f0e",
                "editor.highlighted_line.background": "#C8AE3610",
                "editor.active_line.background": "#C8AE3610",
                "panel.background": "#0f0f0e90",
                "title_bar.background": "#0f0f0e90",
                "title_bar.inactive_background": "#0f0f0e90",
                "status_bar.background": "#0f0f0e90",
                "drop_target.background": "#0f0f0e90",
                "elevated_surface.background": "#0f0f0e",
                "toolbar.background": "#0f0f0e90",
                "tab_bar.background": "#0f0f0e90",
                "tab.inactive_background": "#0f0f0e90",
                "tab.active_background": "#C8AE3630",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0f0f0e",
                "scrollbar.thumb.background": "#C8AE36",
                "editor.gutter.background": "#0f0f0e90",
                "terminal.background": "#0f0f0e10",
                "terminal.foreground": "#c3c3c2",
                "terminal.dim_foreground": "#c3c3c2",
                "terminal.bright_foreground": "#c3c3c2",
                "terminal.ansi.black": "#0f0f0e",
                "terminal.ansi.red": "#C8AE36",
                "terminal.ansi.green": "#8D8A76",
                "terminal.ansi.yellow": "#788486",
                "terminal.ansi.blue": "#8F9187",
                "terminal.ansi.magenta": "#ADAD95",
                "terminal.ansi.cyan": "#AEAF99",
                "terminal.ansi.white": "#c3c3c2",
                "terminal.ansi.bright_black": "#6c6c58",
                "terminal.ansi.bright_red": "#C8AE36",
                "terminal.ansi.bright_green": "#8D8A76",
                "terminal.ansi.bright_yellow": "#788486",
                "terminal.ansi.bright_blue": "#8F9187",
                "terminal.ansi.bright_magenta": "#ADAD95",
                "terminal.ansi.bright_cyan": "#AEAF99",
                "terminal.ansi.bright_white": "#c3c3c2",
                "modified": "#C8AE36",
                "syntax": {
                    "attribute": {
                        "color": "#C8AE36"
                    },
                    "boolean": {
                      "color": "#8D8A76"
                    },
                    "comment": {
                      "color": "#6c6c58"
                    },
                    "comment.doc": {
                      "color": "#6c6c58"
                    },
                    "constant": {
                      "color": "#8D8A76"
                    },
                    "function": {
                      "color": "#AEAF99"
                    },
                    "keyword": {
                      "color": "#8F9187"
                    },
                    "number": {
                      "color": "#ADAD95"
                    },
                    "operator": {
                      "color": "#8F9187"
                    },
                    "string": {
                      "color": "#C8AE36"
                    },
                    "variable": {
                      "color": "#8D8A76"
                    }
                },
                "players": [
                  {
                    "cursor": "#C8AE36",
                    "background": "#0f0f0e",
                    "selection": "#C8AE3630"
                  }
                ]
            }
        }
    ]
}
