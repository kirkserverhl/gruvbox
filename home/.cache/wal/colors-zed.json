{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#12131390",
                "editor.background": "#12131390",
                "editor.foreground": "#c3c4c4",
                "text": "#c3c4c4",
                "text.muted": "#c3c4c470",
                "text.ignored": "#c3c4c440",
                "text.placeholder": "#c3c4c450",
                "ignored": "#c3c4c430",
                "element.hover": "#c3c4c430",
                "ghost_element.hover": "#749E9230",
                "ghost_element.selected": "#749E9230",
                "ghost_element.active": "#749E9260",
                "border": "#121313",
                "editor.highlighted_line.background": "#749E9210",
                "editor.active_line.background": "#749E9210",
                "panel.background": "#12131390",
                "title_bar.background": "#12131390",
                "title_bar.inactive_background": "#12131390",
                "status_bar.background": "#12131390",
                "drop_target.background": "#12131390",
                "elevated_surface.background": "#121313",
                "toolbar.background": "#12131390",
                "tab_bar.background": "#12131390",
                "tab.inactive_background": "#12131390",
                "tab.active_background": "#749E9230",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#121313",
                "scrollbar.thumb.background": "#749E92",
                "editor.gutter.background": "#12131390",
                "terminal.background": "#12131310",
                "terminal.foreground": "#c3c4c4",
                "terminal.dim_foreground": "#c3c4c4",
                "terminal.bright_foreground": "#c3c4c4",
                "terminal.ansi.black": "#121313",
                "terminal.ansi.red": "#749E92",
                "terminal.ansi.green": "#86A994",
                "terminal.ansi.yellow": "#ADAF94",
                "terminal.ansi.blue": "#C7B899",
                "terminal.ansi.magenta": "#AEC692",
                "terminal.ansi.cyan": "#DFD6AC",
                "terminal.ansi.white": "#c3c4c4",
                "terminal.ansi.bright_black": "#5a6e6e",
                "terminal.ansi.bright_red": "#749E92",
                "terminal.ansi.bright_green": "#86A994",
                "terminal.ansi.bright_yellow": "#ADAF94",
                "terminal.ansi.bright_blue": "#C7B899",
                "terminal.ansi.bright_magenta": "#AEC692",
                "terminal.ansi.bright_cyan": "#DFD6AC",
                "terminal.ansi.bright_white": "#c3c4c4",
                "modified": "#749E92",
                "syntax": {
                    "attribute": {
                        "color": "#749E92"
                    },
                    "boolean": {
                      "color": "#86A994"
                    },
                    "comment": {
                      "color": "#5a6e6e"
                    },
                    "comment.doc": {
                      "color": "#5a6e6e"
                    },
                    "constant": {
                      "color": "#86A994"
                    },
                    "function": {
                      "color": "#DFD6AC"
                    },
                    "keyword": {
                      "color": "#C7B899"
                    },
                    "number": {
                      "color": "#AEC692"
                    },
                    "operator": {
                      "color": "#C7B899"
                    },
                    "string": {
                      "color": "#749E92"
                    },
                    "variable": {
                      "color": "#86A994"
                    }
                },
                "players": [
                  {
                    "cursor": "#749E92",
                    "background": "#121313",
                    "selection": "#749E9230"
                  }
                ]
            }
        }
    ]
}
