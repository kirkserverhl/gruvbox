{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#0f111190",
                "editor.background": "#0f111190",
                "editor.foreground": "#c3c3c3",
                "text": "#c3c3c3",
                "text.muted": "#c3c3c370",
                "text.ignored": "#c3c3c340",
                "text.placeholder": "#c3c3c350",
                "ignored": "#c3c3c330",
                "element.hover": "#c3c3c330",
                "ghost_element.hover": "#B3654330",
                "ghost_element.selected": "#B3654330",
                "ghost_element.active": "#B3654360",
                "border": "#0f1111",
                "editor.highlighted_line.background": "#B3654310",
                "editor.active_line.background": "#B3654310",
                "panel.background": "#0f111190",
                "title_bar.background": "#0f111190",
                "title_bar.inactive_background": "#0f111190",
                "status_bar.background": "#0f111190",
                "drop_target.background": "#0f111190",
                "elevated_surface.background": "#0f1111",
                "toolbar.background": "#0f111190",
                "tab_bar.background": "#0f111190",
                "tab.inactive_background": "#0f111190",
                "tab.active_background": "#B3654330",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#0f1111",
                "scrollbar.thumb.background": "#B36543",
                "editor.gutter.background": "#0f111190",
                "terminal.background": "#0f111110",
                "terminal.foreground": "#c3c3c3",
                "terminal.dim_foreground": "#c3c3c3",
                "terminal.bright_foreground": "#c3c3c3",
                "terminal.ansi.black": "#0f1111",
                "terminal.ansi.red": "#B36543",
                "terminal.ansi.green": "#B7934F",
                "terminal.ansi.yellow": "#66938C",
                "terminal.ansi.blue": "#9A9D8B",
                "terminal.ansi.magenta": "#85A597",
                "terminal.ansi.cyan": "#BCAD93",
                "terminal.ansi.white": "#c3c3c3",
                "terminal.ansi.bright_black": "#596d6d",
                "terminal.ansi.bright_red": "#B36543",
                "terminal.ansi.bright_green": "#B7934F",
                "terminal.ansi.bright_yellow": "#66938C",
                "terminal.ansi.bright_blue": "#9A9D8B",
                "terminal.ansi.bright_magenta": "#85A597",
                "terminal.ansi.bright_cyan": "#BCAD93",
                "terminal.ansi.bright_white": "#c3c3c3",
                "modified": "#B36543",
                "syntax": {
                    "attribute": {
                        "color": "#B36543"
                    },
                    "boolean": {
                      "color": "#B7934F"
                    },
                    "comment": {
                      "color": "#596d6d"
                    },
                    "comment.doc": {
                      "color": "#596d6d"
                    },
                    "constant": {
                      "color": "#B7934F"
                    },
                    "function": {
                      "color": "#BCAD93"
                    },
                    "keyword": {
                      "color": "#9A9D8B"
                    },
                    "number": {
                      "color": "#85A597"
                    },
                    "operator": {
                      "color": "#9A9D8B"
                    },
                    "string": {
                      "color": "#B36543"
                    },
                    "variable": {
                      "color": "#B7934F"
                    }
                },
                "players": [
                  {
                    "cursor": "#B36543",
                    "background": "#0f1111",
                    "selection": "#B3654330"
                  }
                ]
            }
        }
    ]
}
