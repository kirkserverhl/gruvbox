{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#12141490",
                "editor.background": "#12141490",
                "editor.foreground": "#c3c4c4",
                "text": "#c3c4c4",
                "text.muted": "#c3c4c470",
                "text.ignored": "#c3c4c440",
                "text.placeholder": "#c3c4c450",
                "ignored": "#c3c4c430",
                "element.hover": "#c3c4c430",
                "ghost_element.hover": "#DE9D1F30",
                "ghost_element.selected": "#DE9D1F30",
                "ghost_element.active": "#DE9D1F60",
                "border": "#121414",
                "editor.highlighted_line.background": "#DE9D1F10",
                "editor.active_line.background": "#DE9D1F10",
                "panel.background": "#12141490",
                "title_bar.background": "#12141490",
                "title_bar.inactive_background": "#12141490",
                "status_bar.background": "#12141490",
                "drop_target.background": "#12141490",
                "elevated_surface.background": "#121414",
                "toolbar.background": "#12141490",
                "tab_bar.background": "#12141490",
                "tab.inactive_background": "#12141490",
                "tab.active_background": "#DE9D1F30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#121414",
                "scrollbar.thumb.background": "#DE9D1F",
                "editor.gutter.background": "#12141490",
                "terminal.background": "#12141410",
                "terminal.foreground": "#c3c4c4",
                "terminal.dim_foreground": "#c3c4c4",
                "terminal.bright_foreground": "#c3c4c4",
                "terminal.ansi.black": "#121414",
                "terminal.ansi.red": "#DE9D1F",
                "terminal.ansi.green": "#D89921",
                "terminal.ansi.yellow": "#918B54",
                "terminal.ansi.blue": "#497E81",
                "terminal.ansi.magenta": "#3A838D",
                "terminal.ansi.cyan": "#458588",
                "terminal.ansi.white": "#c3c4c4",
                "terminal.ansi.bright_black": "#5a6f6f",
                "terminal.ansi.bright_red": "#DE9D1F",
                "terminal.ansi.bright_green": "#D89921",
                "terminal.ansi.bright_yellow": "#918B54",
                "terminal.ansi.bright_blue": "#497E81",
                "terminal.ansi.bright_magenta": "#3A838D",
                "terminal.ansi.bright_cyan": "#458588",
                "terminal.ansi.bright_white": "#c3c4c4",
                "modified": "#DE9D1F",
                "syntax": {
                    "attribute": {
                        "color": "#DE9D1F"
                    },
                    "boolean": {
                      "color": "#D89921"
                    },
                    "comment": {
                      "color": "#5a6f6f"
                    },
                    "comment.doc": {
                      "color": "#5a6f6f"
                    },
                    "constant": {
                      "color": "#D89921"
                    },
                    "function": {
                      "color": "#458588"
                    },
                    "keyword": {
                      "color": "#497E81"
                    },
                    "number": {
                      "color": "#3A838D"
                    },
                    "operator": {
                      "color": "#497E81"
                    },
                    "string": {
                      "color": "#DE9D1F"
                    },
                    "variable": {
                      "color": "#D89921"
                    }
                },
                "players": [
                  {
                    "cursor": "#DE9D1F",
                    "background": "#121414",
                    "selection": "#DE9D1F30"
                  }
                ]
            }
        }
    ]
}
