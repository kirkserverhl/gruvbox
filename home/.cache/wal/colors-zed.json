{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#10101290",
                "editor.background": "#10101290",
                "editor.foreground": "#c3c3c3",
                "text": "#c3c3c3",
                "text.muted": "#c3c3c370",
                "text.ignored": "#c3c3c340",
                "text.placeholder": "#c3c3c350",
                "ignored": "#c3c3c330",
                "element.hover": "#c3c3c330",
                "ghost_element.hover": "#817D6930",
                "ghost_element.selected": "#817D6930",
                "ghost_element.active": "#817D6960",
                "border": "#101012",
                "editor.highlighted_line.background": "#817D6910",
                "editor.active_line.background": "#817D6910",
                "panel.background": "#10101290",
                "title_bar.background": "#10101290",
                "title_bar.inactive_background": "#10101290",
                "status_bar.background": "#10101290",
                "drop_target.background": "#10101290",
                "elevated_surface.background": "#101012",
                "toolbar.background": "#10101290",
                "tab_bar.background": "#10101290",
                "tab.inactive_background": "#10101290",
                "tab.active_background": "#817D6930",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#101012",
                "scrollbar.thumb.background": "#817D69",
                "editor.gutter.background": "#10101290",
                "terminal.background": "#10101210",
                "terminal.foreground": "#c3c3c3",
                "terminal.dim_foreground": "#c3c3c3",
                "terminal.bright_foreground": "#c3c3c3",
                "terminal.ansi.black": "#101012",
                "terminal.ansi.red": "#817D69",
                "terminal.ansi.green": "#979274",
                "terminal.ansi.yellow": "#90908F",
                "terminal.ansi.blue": "#B2AF8E",
                "terminal.ansi.magenta": "#C3BD90",
                "terminal.ansi.cyan": "#D7D19F",
                "terminal.ansi.white": "#c3c3c3",
                "terminal.ansi.bright_black": "#59596d",
                "terminal.ansi.bright_red": "#817D69",
                "terminal.ansi.bright_green": "#979274",
                "terminal.ansi.bright_yellow": "#90908F",
                "terminal.ansi.bright_blue": "#B2AF8E",
                "terminal.ansi.bright_magenta": "#C3BD90",
                "terminal.ansi.bright_cyan": "#D7D19F",
                "terminal.ansi.bright_white": "#c3c3c3",
                "modified": "#817D69",
                "syntax": {
                    "attribute": {
                        "color": "#817D69"
                    },
                    "boolean": {
                      "color": "#979274"
                    },
                    "comment": {
                      "color": "#59596d"
                    },
                    "comment.doc": {
                      "color": "#59596d"
                    },
                    "constant": {
                      "color": "#979274"
                    },
                    "function": {
                      "color": "#D7D19F"
                    },
                    "keyword": {
                      "color": "#B2AF8E"
                    },
                    "number": {
                      "color": "#C3BD90"
                    },
                    "operator": {
                      "color": "#B2AF8E"
                    },
                    "string": {
                      "color": "#817D69"
                    },
                    "variable": {
                      "color": "#979274"
                    }
                },
                "players": [
                  {
                    "cursor": "#817D69",
                    "background": "#101012",
                    "selection": "#817D6930"
                  }
                ]
            }
        }
    ]
}
